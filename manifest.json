{
  "name": "Right Click Menu",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "DEMO JavaScript inection Google Chrome Extension",
  "homepage_url": "http://www.whak.com/",
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
    "https://*/*",
    "http://*/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}
